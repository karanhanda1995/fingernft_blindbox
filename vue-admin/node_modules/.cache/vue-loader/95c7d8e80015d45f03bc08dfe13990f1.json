AACA;AACA;AACA;AACA;AACA;AACA","file":"language.vue","sourceRoot":"src/views/profile","sourcesContent":["<template>\n  <div class=\"language-body\">\n      <span>{{$t('language.choose')}}:</span>\n    <el-select @change=\"change\" v-model=\"value\" class=\"select\" :placeholder=\"$t('language.choose')\">\n      <el-option v-for=\"item in options\" :key=\"item.value\" :label=\"item.label\" :value=\"item.value\" />\n    </el-select>\n  </div>\n</template>\n<script>\nexport default {\n  data() {\n    return {\n      value: \"\",\n      options: [\n        {\n          value: \"en\",\n          label: 'English'\n        },\n        {\n          value: \"cn\",\n          label: '中文'\n        },\n      ],\n    };\n  },\n  methods: {\n    change() {\n      if (this.value == \"en\") {\n        localStorage.setItem(\"locale\", \"en\");\n        this.$i18n.locale = localStorage.getItem(\"locale\");\n      } else if (this.value == \"cn\") {\n        localStorage.setItem(\"locale\", \"zh\");\n        this.$i18n.locale = localStorage.getItem(\"locale\");\n      }\n    },\n  },\n};\n</script>\n<style lang=\"scss\" scoped>\n.language-body{\n    width:100%;\n    height:100%;\n    padding-left:50px;\n    .select{\n        margin:50px 0 0 10px;\n    }\n}\n</style>"]}]}
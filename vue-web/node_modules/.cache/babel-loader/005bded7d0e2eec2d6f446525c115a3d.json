eAJQ,6BAIW;MACjB,OAAO,KAAKF,MAAL,CAAYG,OAAZ,CAAoBC,mBAApB,EAAP;IACD,CANO;IAORC,YAPQ,0BAOQ;MAAA;;MACd,IAAIC,QAAO,GAAI,KAAKN,MAAL,CAAYG,OAAZ,CAAoBI,QAApB,0BAA6B,KAAKL,eAAlC,0DAA6B,sBAAsBM,OAAtB,CAA8BC,iBAA9B,EAA7B,CAAf;MACA,IAAI,CAACH,QAAL,EAAe,OAAO;QAAEI,OAAO,EAAE;MAAX,CAAP;MACf,IAAIA,OAAM,GAAI,KAAKV,MAAL,CAAYG,OAAZ,CAAoBQ,UAApB,CAA+B,KAAKT,eAAL,CAAqBM,OAApD,CAAd;MACAE,OAAM,GAAI,CAACA,OAAD,GAAW,CAAX,GAAeA,OAAzB;MACA,OAAO;QACLA,OAAO,EAAE,KAAKE,MAAL,CAAYC,OAAZ,CAAoBH,OAApB,EAA6B,CAA7B;MADJ,CAAP;IAGD;EAfO,CAFG;EAmBbI,OAAO,EAAE;IACPC,OADO,qBACI;MACT,KAAKC,OAAL,CAAaC,IAAb,CAAkB;QAAEpB,IAAI,EAAE;MAAR,CAAlB;IACD,CAHM;IAIPqB,SAJO,uBAIM;MACX,IAAI,CAAC,KAAKN,MAAL,CAAYO,SAAZ,EAAL,EAA8B;MAC9B,KAAKH,OAAL,CAAaC,IAAb,CAAkB,UAAlB;IACD,CAPM;IAQPG,aARO,2BAQU;MACf,KAAKR,MAAL,CAAYS,OAAZ,CAAoB,KAAKC,EAAL,CAAQ,qBAAR,CAApB,EAAoD,SAApD;IACD,CAVM;IAWPC,WAXO,yBAWQ;MACb,KAAKX,MAAL,CAAYS,OAAZ,CAAoB,KAAKC,EAAL,CAAQ,mBAAR,CAApB;IACD,CAbM;IAcPE,MAdO,oBAcG;MACR,KAAKC,KAAL,CAAWC,UAAX;IACD;EAhBM;AAnBI,CAAf","names":["name","computed","user","$store","state","defaultPaytoken","getters","defaultSalePayToken","erc20Balance","paytoken","payToken","address","toLocaleLowerCase","balance","getBalance","$tools","decimal","methods","goItems","$router","push","goProfile","needLogin","onSuccessCopy","message","$t","onErrorCopy","logout","$web3","disconnect"],"sourceRoot":"","sources":["C:\\Users\\Mohamed Tamim\\Desktop\\SONGTRADER-V2-FULL\\fingernft_blindbox\\fingernft_blindbox\\vue-web\\src\\components\\ProfilePopover.vue"],"sourcesContent":["<template>\n  <el-popover placement=\"bottom-start\" trigger=\"click\" :show-arrow=\"false\" popper-class=\"profile-popover\" :offset=\"0\">\n    <template #reference>\n      <div class=\"user-avatar\">\n        <avatar class=\"avatar-img\" :imageUrl=\"$filters.fullImageUrl(user.avatar)\" :address=\"user.coinbase\" :imgWidth=\"40\" :imgHeight=\"40\" shape=\"circular\">\n        </avatar>\n      </div>\n    </template>\n    <div class=\"profile-wrapper\">\n      <div class=\"profile-wrapper-inner\">\n        <div class=\"profile-header\">\n          <div class=\"f3left\">\n            <div @click=\"goItems\" class=\"cpointer bfont c3\" v-if=\"user && user.nickname\">\n              {{ user.nickname }}\n            </div>\n            <div class=\"cpointer c6\">\n              <span @click=\"goItems\"> {{ $filters.ellipsisAddress(user.coinbase) }} </span>\n              <span class=\"el-icon-copy-document m-left-5\" v-clipboard:copy=\"user.coinbase\" v-clipboard:success=\"onSuccessCopy\" v-clipboard:error=\"onErrorCopy\"></span>\n            </div>\n            <div @click=\"goProfile\" class=\"cpointer cprimary bfont f12\" v-if=\"!user || !user.nickname\">\n              {{ $t('navigation.sdn')}}\n            </div>\n          </div>\n          <div class=\"f1right\">\n            <avatar @click=\"goItems\" class=\"cpointer avatar-img\" :imageUrl=\"$filters.fullImageUrl(user.avatar)\" :address=\"user.coinbase\" :imgWidth=\"40\" :imgHeight=\"40\">\n            </avatar>\n          </div>\n        </div>\n\n        <div class=\"paytoken-item\" v-if=\"defaultPaytoken\">\n          <avatar class=\"paytoken-img\" :imageUrl=\"$filters.fullImageUrl(defaultPaytoken.avatar)\" :address=\"defaultPaytoken.address\" :imgWidth=\"35\" :imgHeight=\"35\"></avatar>\n          <div class=\"m-left-10\">\n            <div class=\"paytoken-t\">\n              {{ $t(\"navigation.balance\") }}\n            </div>\n            <div class=\"paytoken-v\">\n              <div class=\"c3\">\n                {{ erc20Balance.balance }}\n                {{ defaultPaytoken.symbol }}\n              </div>\n            </div>\n          </div>\n        </div>\n\n      </div>\n      <div class=\"profile-line\"></div>\n      <router-link class=\"link-item\" to=\"/items\">\n        {{ $t(\"navigation.myItems\") }}</router-link>\n      <div class=\"link-item\" @click=\"goProfile\">\n        {{ $t(\"navigation.editProfile\") }}\n      </div>\n      <div class=\"link-item\" @click=\"logout\">\n        {{ $t(\"navigation.disconnect\") }}\n      </div>\n    </div>\n  </el-popover>\n\n</template>\n<script>\n  export default {\n    name: \"ProfilePopover\",\n    computed: {\n      user: function () {\n        return this.$store.state.user;\n      },\n      defaultPaytoken () {\n        return this.$store.getters.defaultSalePayToken();\n      },\n      erc20Balance () {\n        let paytoken = this.$store.getters.payToken(this.defaultPaytoken?.address.toLocaleLowerCase());\n        if (!paytoken) return { balance: 0 };\n        let balance = this.$store.getters.getBalance(this.defaultPaytoken.address);\n        balance = !balance ? 0 : balance;\n        return {\n          balance: this.$tools.decimal(balance, 3)\n        };\n      },\n    },\n    methods: {\n      goItems () {\n        this.$router.push({ name: \"Items\" });\n      },\n      goProfile () {\n        if (!this.$tools.needLogin()) return;\n        this.$router.push(\"/profile\");\n      },\n      onSuccessCopy () {\n        this.$tools.message(this.$t(\"request.copySuccess\"), \"success\");\n      },\n      onErrorCopy () {\n        this.$tools.message(this.$t(\"request.copyError\"));\n      },\n      logout () {\n        this.$web3.disconnect();\n      },\n    },\n  }\n</script>\n\n<style lang=\"scss\">\n  .profile-popover {\n    width: 290px !important;\n    inset: 0 auto auto 5px !important;\n  }\n</style>\n\n<style lang=\"scss\" scoped>\n  .user-avatar {\n    display: flex;\n    align-items: center;\n  }\n\n  .profile-wrapper {\n    padding: 20px 10px;\n    display: flex;\n    flex-direction: column;\n    .profile-wrapper-inner {\n      display: flex;\n      align-items: center;\n      flex-wrap: wrap;\n    }\n\n    .avatar-img {\n      display: flex;\n    }\n\n    .profile-header {\n      display: flex;\n      width: 100%;\n      padding-bottom: 20px;\n      .coinbase {\n        font-size: 15px;\n        margin-bottom: 5px;\n      }\n      .f3left {\n        display: flex;\n        flex-direction: column;\n        flex: 3;\n      }\n      .f1right {\n        display: flex;\n        flex: 1;\n        justify-content: flex-end;\n      }\n    }\n\n    .paytoken-item {\n      display: flex;\n      align-items: center;\n      padding-bottom: 20px;\n      .paytoken-img {\n        width: 35px;\n        height: 35px;\n        border-radius: 50%;\n      }\n      .paytoken-t {\n        font-size: 12px;\n        font-weight: 400;\n        color: #999;\n      }\n      .paytoken-v {\n        display: flex;\n        flex-direction: column;\n        align-items: flex-start;\n      }\n      .usdt {\n        background: #eeeeee;\n        border-radius: 5px;\n        padding: 0 5px;\n        font-size: 12px;\n        line-height: 20px;\n      }\n    }\n\n    .link-item {\n      cursor: pointer;\n      font-weight: 400;\n      margin-top: 15px;\n    }\n\n    .profile-line {\n      margin: 10px 0;\n      border: 1px solid #eeeeee;\n    }\n  }\n</style>\n\n"]}]}
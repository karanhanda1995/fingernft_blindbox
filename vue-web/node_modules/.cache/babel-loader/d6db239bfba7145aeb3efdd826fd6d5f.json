      </span> -->\n          </span>\n        </div>\n\n      </div>\n      <div class=\"info-right\" v-if=\"full\">\n        <el-button size=\"mini\" @click=\"showAuction\" plain type=\"primary\">\n          {{ $t('details.buy2')}}\n        </el-button>\n      </div>\n\n    </div>\n    <div class=\"countdown\" :class=\" {'full': full }\">\n      <auction-count-down class=\"cgreen\" :startTime=\"parseInt(auction.startTime)\" :endTime=\"parseInt(auction.endTime)\" :expiredTime=\"parseInt(auction.expiredTime)\">\n      </auction-count-down>\n    </div>\n\n  </div>\n</template>\n\n<script>\n  export default {\n    name: \"LowestAuction\",\n    props: {\n      auction: {\n        type: Object,\n        default: {},\n      },\n      myAsset: {\n        type: Object,\n        default: null,\n      },\n      full: {\n        type: Boolean,\n        default: false,\n      },\n    },\n    methods: {\n      goAccount (address) {\n        this.$router.push({ path: \"/account/\" + address });\n      },\n      showAuction () {\n        this.$emit(\"detail\", this.auction);\n      },\n    },\n  }\n</script>\n\n<style lang=\"scss\" scoped>\n  .auction-item {\n    flex: 1;\n    padding-left: 10px;\n    &.full {\n      padding-left: 0;\n      .countdown {\n        padding: 4px 5px;\n      }\n    }\n    .title {\n      display: flex;\n      font-size: 14px;\n      color: #aaa;\n      margin-bottom: 5px;\n      justify-content: space-between;\n    }\n    .info {\n      margin-left: 5px;\n      flex: 1;\n    }\n    .op {\n      background: $secondPrimaryColor;\n      font-weight: normal;\n      font-size: 13px;\n      padding: 2px 5px;\n      border-radius: $borderRadius;\n      margin-left: 5px;\n    }\n    .auction-body {\n      display: flex;\n    }\n    .countdown {\n      background: #f0f0f0;\n      padding: 2px 5px;\n      border-radius: 5px;\n      margin-top: 5px;\n      align-items: center;\n      display: flex;\n      justify-content: center;\n    }\n  }\n</style>\n"]}]}
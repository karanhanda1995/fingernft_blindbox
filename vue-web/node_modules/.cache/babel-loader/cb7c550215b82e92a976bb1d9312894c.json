n\t\tmin-height: auto;\n\t}\n\t.file-box {\n\t\tmargin-top: 10px;\n\t\tbackground: $primaryColor;\n\t\tborder-radius: 8px;\n\t\tpadding: 9px 20px;\n\t\tfont-size: 12px;\n\t\tfont-weight: 400;\n\t\tcolor: #fff;\n\t\tcursor: pointer;\n\t\tposition: relative;\n\t\t.file-btn {\n\t\t\tposition: absolute;\n\t\t\tz-index: 1;\n\t\t\tleft: 0;\n\t\t\tright: 0;\n\t\t\ttop: 0;\n\t\t\tbottom: 0;\n\t\t\topacity: 0;\n\t\t\tcursor: pointer;\n\t\t}\n\t}\n\n\t.chooseFile-Content-RightSection {\n\t\tdisplay: flex;\n\t\tflex-direction: column;\n\t\talign-items: center;\n\t}\n\t@media screen and (max-width: $media_l4) {\n\t\t.profile-wrapper {\n\t\t\twidth: 80%;\n\t\t}\n\t}\n\t@media screen and (max-width: $media_l5) {\n\t\t.profile-wrapper {\n\t\t\twidth: 90%;\n\t\t}\n\t\t.chooseFile-Content {\n\t\t\tflex-direction: column;\n\t\t\t.chooseFile-Content-RightSection {\n\t\t\t\tflex-direction: row;\n\t\t\t\talign-items: flex-end;\n\t\t\t\tmargin-top: 10px;\n\t\t\t\t.file-box {\n\t\t\t\t\tmargin-left: 5px;\n\t\t\t\t\tmargin-top: 0;\n\t\t\t\t}\n\t\t\t}\n\t\t}\n\t}\n\t@media screen and (max-width: $media_l6) {\n\t\t.profile-wrapper {\n\t\t\twidth: 100%;\n\t\t}\n\t}\n</style>\n<style lang=\"scss\">\n\t.unit input {\n\t\tmargin-top: 8px !important;\n\t\theight: 40px !important;\n\t\tbackground: #eeeeee !important;\n\t\tborder-radius: 5px !important;\n\n\t\tfont-size: 14px !important;\n\t\tfont-weight: 400 !important;\n\t\tcolor: #000000 !important;\n\t\tpadding-left: 6px !important;\n\t}\n\t.unit input::placeholder {\n\t\tfont-size: 8px !important;\n\t\tfont-weight: 400 !important;\n\t\tcolor: #000000 !important;\n\t\tline-height: 14px !important;\n\t\topacity: 0.4 !important;\n\t}\n</style>\n"]}]}